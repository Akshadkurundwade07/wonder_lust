
<% layout("/layouts/boilerplate") %>

<body>
    <h1>All Listings </h1>
<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
 <% for(let listing of allListings){ %>
  <a href="/listings/<%= listing._id %>" class="listing">
<div class="card col">
  <img 
  class="card-img-top" 
  src="<%= listing.image %>" 
  alt="Card image cap" 
  style="height: 20rem">
  <div class="card-img-overlay">
    
  </div>
  <div class="card-body">
  <p class="card-text"><%=listing.title %></p>
   &#8377; <%= listing.price.toLocaleString("en-IN") %> / night </p>
  </div>
</div>
</a>
    <% } %>
    
</div>
